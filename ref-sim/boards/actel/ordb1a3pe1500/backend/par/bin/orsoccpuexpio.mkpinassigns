#
# Ethernet SMII and MDC interface
#
ETHERNET_BUS_SETTINGS ?=-iostd LVTTL -RES_PULL None -fixed yes
ETHERNET_OUT_BUS_SETTINGS ?= -REGISTER No -OUT_DRIVE 12 -SLEW High -SKEW Off -OUT_LOAD 35
ETHERNET_OUT_REG_BUS_SETTINGS ?= -REGISTER Yes -OUT_DRIVE 12 -SLEW High -SKEW Off -OUT_LOAD 35
ETH_CLK_PIN ?=$(ORSOCCPUBOARD_ETHCLK_PIN)
# ORSoC Exp. Board SMII
ETH0_MDC_PIN ?=$(ORSOCEXPBOARD_ETHA_MDC_PIN)
ETH0_MDIO_PIN ?=$(ORSOCEXPBOARD_ETHA_MDIO_PIN)
