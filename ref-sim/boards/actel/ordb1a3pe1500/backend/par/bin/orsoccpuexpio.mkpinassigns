# Pin settings for ORSoC CPU board on ORSoC Ethernet I/O board


# Pin number definitions for ORSoC CPU board
include $(PDC_MKPINS_PATH)/orsoccpuboard.mkpins

# Pin definitions and assignments for ORSoC Ethernet I/O expansion board
include $(PDC_MKPINS_PATH)/orsocexpboard.mkpins

# Now actual assigns to variables used in PDC generation

RST_BUS_SETTING ?= -iostd LVTTL -REGISTER No -RES_PULL Up -fixed yes
CLK_BUS_SETTING ?= -iostd LVTTL -REGISTER No -RES_PULL None -fixed yes
RST_PIN ?= $(ORSOCCPUBOARD_RSTN_PIN)
CLK_PIN ?= $(ORSOCCPUBOARD_CLK_PIN)

JTAG_DBG_SETTINGS= -iostd LVTTL -REGISTER No  -fixed yes
JTAG_DBG_TDI_PIN ?= $(ORSOCCPUBOARD_JTAG_DBG_TDI_PIN)
